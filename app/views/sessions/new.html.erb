<h1> Login </h1>

<%= form_with url: login_path do |form| %>
<%= form.label :username %>
<%= form.text_field :username, class: "form-control" %><br>
<%= form.label :password %>
<%= form.password_field :password, class: "form-control"%><br>


<div class="mb-3">
<%= form.submit "Login" %>
</div>
  <p>Don't have an account? <%= link_to 'Sign Up', sign_up_path %> here! <p>


<% end %>
